<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义SiteImgRadioButton 设置图片大小-->
    <declare-styleable name="SiteImgRadioButton">
        <attr name="drawableSize" format="dimension" />
        <attr name="drawableTop" format="reference" />
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
        <attr name="drawableBottom" format="reference" />
    </declare-styleable>

    <!--VPindicator的参数设置-->
    <declare-styleable name="PagerSlidingTab">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="pst_dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackgrounds" format="reference" />
        <attr name="sameLine" format="boolean" />
        <attr name="pst_textAllCaps" format="boolean" />
    </declare-styleable>

    <!--NavigationTabStrip-->
    <declare-styleable name="NavigationTabStrip">
        <attr name="nts_color" format="color"/>
        <attr name="nts_size" format="dimension"/>
        <attr name="nts_weight" format="dimension"/>
        <attr name="nts_factor" format="float"/>
        <attr name="nts_titles" format="reference"/>
        <attr name="nts_bold" value="true"/>
        <attr name="nts_type" format="enum">
            <enum name="line" value="0"/>
            <enum name="point" value="1"/>
        </attr>
        <attr name="nts_gravity" format="enum">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
        </attr>
        <attr name="nts_typeface" format="string"/>
        <attr name="nts_corners_radius" format="dimension"/>
        <attr name="nts_animation_duration" format="integer"/>
        <attr name="nts_inactive_color" format="color"/>
        <attr name="nts_active_color" format="color"/>
    </declare-styleable>

    <!--StateButton-->
    <declare-styleable name="StateButton">
        <!--text color-->
        <!--设置正常显示的文字颜色-->
        <attr name="normalTextColor" format="color|reference" />
        <!--设置按下状态显示的文字颜色-->
        <attr name="pressedTextColor" format="color|reference" />
        <!--设置不可用时（enable = false）显示的文字颜色-->
        <attr name="unableTextColor" format="color|reference" />

        <!--StrokeWidth-->
        <!--设置正常显示的边框宽度-->
        <attr name="normalStrokeWidth" format="dimension|reference" />
        <!--设置按下时显示的边框宽度-->
        <attr name="pressedStrokeWidth" format="dimension|reference" />
        <!--设置不可用时（enable = false）显示的边框宽度-->
        <attr name="unableStrokeWidth" format="dimension|reference" />

        <!--StrokeColor-->
        <!--设置正常显示的边框颜色-->
        <attr name="normalStrokeColor" format="color|reference" />
        <!--设置按下时显示的边框颜色-->
        <attr name="pressedStrokeColor" format="color|reference" />
        <!--设置不可用时（enable = false）显示的边框颜色-->
        <attr name="unableStrokeColor" format="color|reference" />

        <!--background color-->
        <!--设置正常显示的背景颜色-->
        <attr name="normalBackgroundColor" format="color|reference" />
        <!--设置按下状态显示的背景颜色-->
        <attr name="pressedBackgroundColor" format="color|reference" />
        <!--设置不可用时（enable = false）背景颜色-->
        <attr name="unableBackgroundColor" format="color|reference" />

        <!--渐变色-->
        <!--background start color -->
        <!--设置正常显示的背景开始颜色-->
        <attr name="normalBackgroundStartColor" format="color|reference"/>
        <!--设置按下状态显示的背景开始颜色-->
        <attr name="pressedBackgroundStartColor" format="color|reference"/>
        <!--设置不可用时（enable = false）背景开始颜色-->
        <attr name="unableBackgroundStartColor" format="color|reference"/>
        <!--background center color -->
        <!--设置正常显示的背景中间颜色-->
        <attr name="normalBackgroundCenterColor" format="color|reference"/>
        <!--设置按下状态显示的背景中间颜色-->
        <attr name="pressedBackgroundCenterColor" format="color|reference"/>
        <!--设置不可用时（enable = false）背景中间颜色-->
        <attr name="unableBackgroundCenterColor" format="color|reference"/>
        <!--background end color -->
        <!--设置正常显示的背景结束颜色-->
        <attr name="normalBackgroundEndColor" format="color|reference"/>
        <!--设置按下状态显示的背景结束颜色-->
        <attr name="pressedBackgroundEndColor" format="color|reference"/>
        <!--设置不可用时（enable = false）背景结束颜色-->
        <attr name="unableBackgroundEndColor" format="color|reference"/>

        <!--渐变方向-->
        <attr name="normalOrientation">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="pressedOrientation">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="unableOrientation">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <!--类型-->
        <attr name="normalShapeType">
            <enum name="LINEAR_GRADIENT" value="0" />
            <enum name="OVAL" value="1" />
            <enum name="LINE" value="2" />
            <enum name="RING" value="3" />
        </attr>
        <attr name="pressedShapeType">
            <enum name="LINEAR_GRADIENT" value="0" />
            <enum name="OVAL" value="1" />
            <enum name="LINE" value="2" />
            <enum name="RING" value="3" />
        </attr>
        <attr name="unableShapeType">
            <enum name="LINEAR_GRADIENT" value="0" />
            <enum name="OVAL" value="1" />
            <enum name="LINE" value="2" />
            <enum name="RING" value="3" />
        </attr>

        <!--Dash-->
        <!--设置dash width-->
        <attr name="strokeDashWidth" format="dimension|reference" />
        <!--设置dash-->
        <attr name="strokeDashGap" format="dimension|reference" />

        <!--background radius-->
        <attr name="radius" format="dimension|reference" />
        <attr name="round" format="boolean|reference" />
        <!--animation duration-->
        <attr name="animationDuration" format="integer|reference" />
    </declare-styleable>

    <!--StateImageView-->
    <declare-styleable name="StateImageView">
        <attr name="normalBackground" format="color|reference" />
        <attr name="pressedBackground" format="color|reference" />
        <attr name="unableBackground" format="color|reference" />
        <!--animation duration-->
        <attr name="AnimationDuration" format="integer|reference" />
    </declare-styleable>

    <!--RoundImageView-->
    <declare-styleable name="RoundImageView">
        <attr name="is_circle" format="boolean" />
        <attr name="is_cover_src" format="boolean" />
        <attr name="corner_radius" format="dimension" />
        <attr name="corner_top_left_radius" format="dimension" />
        <attr name="corner_top_right_radius" format="dimension" />
        <attr name="corner_bottom_left_radius" format="dimension" />
        <attr name="corner_bottom_right_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="inner_border_width" format="dimension" />
        <attr name="inner_border_color" format="color" />
        <attr name="mask_color" format="color" />
    </declare-styleable>

    <!--SwitchButton-->
    <declare-styleable name="SwitchButton">
        <!--阴影半径-->
        <attr name="sb_shadow_radius" format="reference|dimension" />
        <!--阴影偏移-->
        <attr name="sb_shadow_offset" format="reference|dimension" />
        <!--阴影颜色-->
        <attr name="sb_shadow_color" format="reference|color" />
        <!--关闭颜色-->
        <attr name="sb_uncheck_color" format="reference|color" />
        <!--开启颜色-->
        <attr name="sb_checked_color" format="reference|color" />
        <!--边框宽度-->
        <attr name="sb_border_width" format="reference|dimension" />
        <!--开启指示器颜色-->
        <attr name="sb_checkline_color" format="reference|color" />
        <!--开启指示器线宽-->
        <attr name="sb_checkline_width" format="reference|dimension" />
        <!--关闭指示器颜色-->
        <attr name="sb_uncheckcircle_color" format="reference|color" />
        <!--关闭指示器线宽-->
        <attr name="sb_uncheckcircle_width" format="reference|dimension" />
        <!--关闭指示器半径-->
        <attr name="sb_uncheckcircle_radius" format="reference|dimension" />
        <!--是否选中-->
        <attr name="sb_checked" format="reference|boolean" />
        <!--是否启用阴影-->
        <attr name="sb_shadow_effect" format="reference|boolean" />
        <!--动画时间，默认300ms-->
        <attr name="sb_effect_duration" format="reference|integer" />
        <!--按钮颜色-->
        <attr name="sb_button_color" format="reference|color" />
        <!--是否显示指示器，默认true：显示-->
        <attr name="sb_show_indicator" format="reference|boolean" />
        <!--背景色，默认白色-->
        <attr name="sb_background" format="reference|color" />
        <!--是否启用特效，默认true-->
        <attr name="sb_enable_effect" format="reference|boolean" />
    </declare-styleable>

    <!--VerCodeBorderLayout-->
    <declare-styleable name="VerCodeBorderLayout">
        <attr name="vc_maxNum" format="integer" />
        <attr name="vc_box_width" format="reference" />
        <attr name="vc_box_height" format="reference" />
        <attr name="vc_box_padding" format="reference" />
        <attr name="vc_normalBackground" format="reference" />
        <attr name="vc_pressedBackground" format="reference" />
        <attr name="vc_normalTextColor" format="color|reference" />
        <attr name="vc_pressedTextColor" format="color|reference" />
    </declare-styleable>


    <!--SuperEditText-->
    <declare-styleable name="SuperEditText">
        <!--图片资源-->
        <attr name="clearDrawable" format="reference" />
        <attr name="visibleDrawable" format="reference" />
        <attr name="invisibleDrawable" format="reference" />
        <!--按钮宽度大小-->
        <attr name="BtnWidth" format="dimension" />
        <!--按钮间距大小-->
        <attr name="BtnSpacing" format="dimension" />
        <!--边框颜色和样式-->
        <attr name="styleColor" format="color" />
        <attr name="borderStyle" format="string" />
    </declare-styleable>

    <!--ViewPager3D-->
    <declare-styleable name="CoverFlowViewPager">
        <attr name="overscroll_rotation" format="float" />
        <attr name="swipe_rotation" format="float" />
        <attr name="swipe_translation" format="integer" />
        <attr name="overscroll_translation" format="integer" />
        <attr name="overscroll_animation_duration" format="integer" />
        <attr name="animate_alpha" format="boolean" />
    </declare-styleable>

    <!--ViewPagerIndicator https://github.com/LinweiJ/ViewPagerIndicator-->
    <declare-styleable name="ViewPagerIndicator">
        <attr name="vpi_selected_color" format="color|reference" />
        <attr name="vpi_default_color" format="color|reference" />
        <attr name="vpi_radius" format="dimension|reference" />
        <attr name="vpi_length" format="dimension|reference" />
        <attr name="vpi_distance" format="dimension|reference" />
        <attr name="vpi_num" format="integer" />
        <attr name="vpi_indicatorType">
            <enum name="LINE" value="0" />
            <enum name="CIRCLE" value="1" />
            <enum name="CIRCLE_LINE" value="2" />
            <enum name="BEZIER" value="3" />
            <enum name="SPRING" value="4" />
        </attr>
        <attr name="vpi_distanceType">
            <enum name="BY_RADIUS" value="0" />
            <enum name="BY_DISTANCE" value="1" />
            <enum name="BY_LAYOUT" value="2" />
        </attr>
        <attr name="vpi_animation" format="boolean" />
    </declare-styleable>

    <!--SoftRadioButton-->
    <declare-styleable name="SoftRadioButton">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColorChecked" format="color" />
        <attr name="textColor" format="color" />
        <attr name="upImage" format="reference" />
        <attr name="downImage" format="reference" />
        <attr name="upImageChecked" format="reference" />
        <attr name="downImageChecked" format="reference" />
        <attr name="rightImgCheck" format="reference" />
        <attr name="rightImgChecked" format="reference" />
    </declare-styleable>


    <!--ArcProgressBar-->
    <declare-styleable name="ArcProgressBar">
        <attr name="arc_max_progress" format="float"/>
        <attr name="arc_bg_color" format="color"/>
        <attr name="arc_stroke_width" format="dimension"/>
        <attr name="arc_progress" format="float"/>
        <attr name="arc_progress_color" format="color"/>
        <attr name="arc_first_text" format="string"/>
        <attr name="arc_first_text_color" format="color"/>
        <attr name="arc_first_text_size" format="dimension"/>
        <attr name="arc_second_text" format="string"/>
        <attr name="arc_second_text_color" format="color"/>
        <attr name="arc_second_text_size" format="dimension"/>
        <attr name="arc_angle_size" format="float"/>
        <attr name="arc_start_angle" format="float"/>
    </declare-styleable>


    <!-- 带下划线的TextView -->
    <declare-styleable name="BorderTextView">
        <attr name="layout_borders" format="boolean"/>
        <attr name="layout_borderLeft" format="boolean"/>
        <attr name="layout_borderTop" format="boolean"/>
        <attr name="layout_borderRight" format="boolean"/>
        <attr name="layout_borderBottom" format="boolean"/>
    </declare-styleable>

    <!--CircleProgressView-->
    <declare-styleable name="CircleProgressView">
        <!--画笔宽度-->
        <attr name="progress_paint_width" format="dimension" />
        <!--画笔颜色-->
        <attr name="progress_paint_color" format="color" />
        <!--加载进度的开始位置-->
        <attr name="location" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
    </declare-styleable>

    <!--CNToolbar-->
    <declare-styleable name="CNToolbar">
        <attr name="cn_leftText" format="string"/>
        <attr name="cn_leftImage" format="reference"/>
        <attr name="cn_titleText" format="string"/>
        <attr name="cn_rightText" format="string"/>
        <attr name="cn_rightImage" format="reference"/>
        <attr name="cn_background" format="reference"/>
        <attr name="cn_textSize" format="integer"/>
        <attr name="cn_titleTextSize" format="integer"/>
        <attr name="cn_textColor" format="color"/>
    </declare-styleable>


    <!--自定义TextView 数字闪动-->
    <declare-styleable name="RunNumTextView">
        <attr name="tv_duration" format="integer" />
        <attr name="tv_number" format="float" />
        <attr name="tv_is_auto" format="boolean" />
    </declare-styleable>
   <!-- <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"> </attr>
        <attr name="textColorx" format="color" />
        <attr name="textSizex" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>-->
    <!--圆圈进度-->
    <declare-styleable name="CircleProgressBar">
        <!--进度值-->
        <attr name="progress" format="integer" />
        <!--进度最大值-->
        <attr name="maxx" format="integer" />
        <!--进度颜色-->
        <attr name="progressColor" format="color" />
        <!--进度文字-->
        <attr name="progressTextVisibility" format="enum">
            <enum name="visible" value="0"></enum>
            <enum name="invisible" value="4"></enum>
        </attr>
        <!--进度文字-->
        <attr name="progressTextSize" format="dimension" />
        <!--进度文字颜色-->
        <attr name="progressTextColor" format="color" />
        <!--进度背景色-->
        <attr name="progressBackgroundColor" format="color" />
        <!--进度开始角度 0-360-->
        <attr name="progressStartAngle" format="float" />
        <!--圆圈线宽度-->
        <attr name="progressStrokeWidth" format="dimension" />
        <!--圆圈半径-->
        <attr name="progressRadius" format="dimension" />
    </declare-styleable>

</resources>